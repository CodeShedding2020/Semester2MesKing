@model IEnumerable<Messenger_Kings.Models.Points>

@{
    ViewBag.Title = "Index";
}

<h2>My Points</h2>

<table class="table">
    <tr>
 
        <th>
            @Html.DisplayNameFor(model => model.ClientPoints)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
    
        <td>
            @Html.DisplayFor(modelItem => item.ClientPoints)
        </td>
    </tr>
}

</table>
